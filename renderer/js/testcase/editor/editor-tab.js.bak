    // æ˜¾ç¤ºå‘½ä»¤é€‰æ‹©èœå•
    showCommandMenu(insertArea, insertIndex) {
        console.log(`showCommandMenu è¢«è°ƒç”¨ï¼Œæ’å…¥ä½ç½®: ${insertIndex}, æ’å…¥åŒºåŸŸå­˜åœ¨: ${!!insertArea}`);
        
        if (this.isTestRunning) {
