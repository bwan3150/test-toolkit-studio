    // å…¬å…±API - è®¾ç½®æ–‡ä»¶è·¯å¾„å¹¶åŠ è½½å†…å®¹
    async setFile(filePath) {
        try {
            // åˆ›å»ºTKEç¼“å†²åŒº
            this.buffer = new window.TKEEditorBuffer(filePath);
            await this.buffer.initialize();
            
            // è®¾ç½®ç›‘å¬å™¨
            this.buffer.addListener((eventType, data) => {
                this.handleBufferEvent(eventType, data);
            });
            
            // åŠ è½½æ–‡ä»¶å†…å®¹
            await this.buffer.loadFromFile();
            
            window.rLog(`ğŸ“ EditorTabæ–‡ä»¶è®¾ç½®å®Œæˆ: ${filePath}`);
        } catch (error) {
            window.rError(`âŒ EditorTabè®¾ç½®æ–‡ä»¶å¤±è´¥: ${error.message}`, error);
            throw error; // é‡æ–°æŠ›å‡ºé”™è¯¯ä¾›ä¸Šå±‚å¤„ç†
        }
    }
    
    // å¤„ç†ç¼“å†²åŒºäº‹ä»¶
    handleBufferEvent(eventType, data) {
        switch (eventType) {
            case 'loaded':
            case 'content-changed':
                this.render(); // é‡æ–°æ¸²æŸ“
                break;
            case 'saved':
                window.rLog('ğŸ’¾ æ–‡ä»¶å·²ä¿å­˜');
                break;
        }
    }
    
    getValue() {
        if (!this.buffer) {
            window.rError('âŒ TKEç¼“å†²åŒºæœªåˆå§‹åŒ–');
            return '';
        }
        
        // å¦‚æœæ˜¯æ–‡æœ¬æ¨¡å¼ä¸”æœ‰DOMæ›´æ”¹ï¼Œå…ˆåŒæ­¥åˆ°ç¼“å†²åŒº
        if (this.currentMode === 'text' && this.textContentEl) {
            const domContent = this.textContentEl.textContent || '';
            const bufferContent = this.buffer.getRawContent();
            
            if (domContent !== bufferContent) {
                window.rLog('ğŸ“ æ–‡æœ¬æ¨¡å¼å†…å®¹æœ‰å˜æ›´ï¼ŒåŒæ­¥åˆ°ç¼“å†²åŒº');
                // å¼‚æ­¥æ›´æ–°ç¼“å†²åŒºï¼Œä½†ç«‹å³è¿”å›DOMå†…å®¹
                this.buffer.updateFromText(domContent).catch(error => {
                    window.rError(`âŒ åŒæ­¥æ–‡æœ¬å†…å®¹å¤±è´¥: ${error.message}`);
                });
                return domContent;
            }
        }
        
        // ä»ç¼“å†²åŒºè·å–æœ€æ–°å†…å®¹
        const content = this.buffer.getRawContent();
        window.rLog(`ğŸ“– ä»TKEç¼“å†²åŒºè·å–å†…å®¹é•¿åº¦: ${content.length}`);
        return content;
    }
    
